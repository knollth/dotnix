{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#cfbafa",
          "style": "plain",
          "template": "<b>{{ .UserName }}<b> ",
          "type": "session"
        },
        {
          "foreground": "#bcbdc1",
          "style": "plain",
          "template": "in ",
          "type": "text"
        },
        {
          "foreground": "#90e1c6",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "<b>{{ .Path }}<b> ",
          "type": "path"
        },
        {
          "foreground": "#5FD7FF",
          "properties": {
            "branch_ahead_icon": "",
            "branch_behind_icon": "",
            "branch_gone_icon": "",
            "branch_icon": "",
            "branch_identical_icon": "",
            "cherry_pick_icon": "",
            "commit_icon": "",
            "fetch_status": true,
            "merge_icon": "",
            "rebase_icon": "",
            "revert_icon": "",
            "tag_icon": ""
          },
          "style": "plain",
          "template": "<#ffffff>on</> {{ .HEAD }}{{ if .Staging.Changed }}<#87FF00> \u25cf {{ .Staging.String }}</>{{ end }}{{ if .Working.Changed }}<#D75F00> \u25cf {{ .Working.String }}</>{{ end }} ",
          "type": "git"
        },
        {
          "foreground": "#73d0ff",
          "style": "plain",
          "template": "ó±„… ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 3
}
